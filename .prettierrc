{
	"plugins": ["@trivago/prettier-plugin-sort-imports"],
	"tabWidth": 2,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"semi": false,
	"trailingComma": "none",
	"useTabs": true,
	"importOrder": [
		"^components/(.*)$",
		"^utils/(.*)$",
		"^http/(.*)$",
		"^store/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
